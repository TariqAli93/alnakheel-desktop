    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "sass": "^1.93.0",
    "tailwindcss": "^4.1.13",
    "vite": "^7.0.5",
    "vue": "^3.5.17",
    "vue-eslint-parser": "^10.2.0"
