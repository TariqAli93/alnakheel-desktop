    "electron-devtools-installer": "^4.0.0",
    "electron-updater": "^6.6.2",
    "jwt-decode": "^4.0.0",
    "lucide-vue-next": "^0.523.0",
    "mitt": "^3.0.1",
    "moment": "^2.30.1",
    "pinia": "^3.0.3",
    "swiper": "^11.2.10",
    "vue-easy-lightbox": "^1.19.0",
